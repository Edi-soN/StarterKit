package com.capgemini.java;

public class Cell {
	private int positionX;
	private int positionY;
	private int numberOfAliveNeighbors;
	private int life;
	
	public Cell(int positionX, int positionY, int life){
		this.positionX = positionX;
		this.positionY = positionY;
		this.life = life;
		this.numberOfAliveNeighbors = 0;
	}
}
